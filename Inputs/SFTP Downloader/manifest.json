{
    "@visokiotype": "CustomBlockSchema.CustomBlockManifest",
    "name": "SFTP Downloader",
    "scriptFilename": "script.py",
    "language": "PYTHON",
    "executableVersion": null,
    "minVersions": [
        null
    ],
    "optionsVersion": 1,
    "apiVersion": "VERSION_0",
    "isResourceIntensiveScript": false,
    "showPartitioning": false,
    "icon": "data:image/png;base64,",
    "description": "Download files from a SFTP server folder.",
    "category": "Inputs",
    "subcategory": null,
    "tags": [
        "sftp",
        "download"
    ],
    "introductoryText": "### Download all files ending with the specified extension contained in a folder on a SFTP server\n#### Connect this block to an 'Append files' block configured to 'use file names from the upstream block' to download files and load their content in Omniscope.\n##### See live example [here](https://omniscope.me/internal/Forums/Custom+block/SFTP.iox/)",
    "dependencies": "pysftp",
    "options": [
        {
            "name": "username",
            "title": "Username",
            "description": null,
            "groupTitle": "Authentication",
            "width": "ONE",
            "@visokiotype": "CustomBlockSchema.TextCustomBlockPublicOption",
            "mandatory": false,
            "defaultValue": "demo"
        },
        {
            "name": "password",
            "title": "Password",
            "description": null,
            "groupTitle": "Authentication",
            "width": "ONE",
            "@visokiotype": "CustomBlockSchema.PasswordCustomBlockPublicOption",
            "mandatory": false
        },
        {
            "name": "hostname",
            "title": "Hostname",
            "description": null,
            "groupTitle": "Host",
            "width": "ONE",
            "@visokiotype": "CustomBlockSchema.TextCustomBlockPublicOption",
            "mandatory": true,
            "defaultValue": "test.rebex.net"
        },
        {
            "name": "port",
            "title": "Port",
            "description": null,
            "groupTitle": "Host",
            "width": "HALF",
            "@visokiotype": "CustomBlockSchema.IntegerCustomBlockPublicOption",
            "numMin": null,
            "numMax": null,
            "mandatory": true,
            "defaultValue": 22,
            "showAsSlider": false
        },
        {
            "name": "folder",
            "title": "Folder Path",
            "description": "The path to the folder e.g.. /var/myfolder/",
            "groupTitle": "Folder",
            "width": "ONE",
            "@visokiotype": "CustomBlockSchema.TextCustomBlockPublicOption",
            "mandatory": true,
            "defaultValue": "/"
        },
        {
            "name": "extension",
            "title": "File Extension (optional)",
            "description": "File extension / suffix for files to download. If empty all files will be download.",
            "groupTitle": "Folder",
            "width": "SHRINK",
            "@visokiotype": "CustomBlockSchema.TextCustomBlockPublicOption",
            "mandatory": false,
            "defaultValue": "txt"
        }
    ],
    "blockOutputs": [
        {
            "@visokiotype": "CustomBlockSchema.BlockOutputPublicOption",
            "id": "Output Data",
            "label": "1",
            "displayName": "Output 1",
            "tooltip": null
        }
    ],
    "docker": {
        "@visokiotype": "CustomBlockSchema.DockerCustomBlockPublicOption",
        "customBaseImage": null,
        "useCustomBaseImage": false,
        "customSystemLibraries": null,
        "installVisokioRepLibraries": false
    },
    "designLock": false,
    "apiMode": "BATCH"
}